<?xml version="1.0" ?><resources>

    <!-- Base application theme. -->
    <style name="ui_navigator_AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/ui_navigator_colorPrimary</item>
        <item name="colorPrimaryDark">@color/ui_navigator_colorPrimaryDark</item>
        <item name="colorAccent">@color/ui_navigator_colorAccent</item>
    </style>

    <style name="ui_navigator_AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="ui_navigator_AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>

    <style name="ui_navigator_AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>

</resources>